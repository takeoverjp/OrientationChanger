<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="xyz.takeoverjp.orientationchanger">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UndefinedActivity" />
        <activity
            android:name=".UnspecifiedActivity"
            android:screenOrientation="unspecified"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".BehindActivity"
            android:screenOrientation="behind"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".PortraitActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".LandscapeActivity"
            android:screenOrientation="landscape"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".ReversePortraitActivity"
            android:screenOrientation="reversePortrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".ReverseLandscapeActivity"
            android:screenOrientation="reverseLandscape"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".SensorPortraitActivity"
            android:screenOrientation="sensorPortrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".SensorLandscapeActivity"
            android:screenOrientation="sensorLandscape"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".UserPortraitActivity"
            android:screenOrientation="userPortrait"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".UserLandscapeActivity"
            android:screenOrientation="userLandscape"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".SensorActivity"
            android:screenOrientation="sensor"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".FullSensorActivity"
            android:screenOrientation="fullSensor"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".NosensorActivity"
            android:screenOrientation="nosensor"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".UserActivity"
            android:screenOrientation="user"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".FullUserActivity"
            android:screenOrientation="fullUser"
            tools:ignore="LockedOrientationActivity" />
        <activity
            android:name=".LockedActivity"
            android:screenOrientation="locked"
            tools:ignore="LockedOrientationActivity" />
    </application>

</manifest>